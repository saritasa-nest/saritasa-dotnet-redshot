{
  "NLog": {
    "targets": {
      "allfile": {
        "type": "File",
        "fileName": "${RedShotBinaryFolder}/logs/${date:format=yyyy-MM-dd\\:HH-mm-ss:cached=true}.log"
      },
      "ffmpegDebug": {
        "type": "File",
        "fileName": "${RedShotBinaryFolder}/logs/ffmpeg-${date:format=yyyy-MM-dd\\:HH-mm-ss:cached=true}.log"
      },
      "CLIdebug": {
        "type": "File",
        "fileName": "${RedShotBinaryFolder}/logs/cli-${date:format=yyyy-MM-dd\\:HH-mm-ss:cached=true}.log"
      }
    },
    "rules": [
      {
        "logger": "CLIdebug",
        "minLevel": "Trace",
        "writeTo": "CLIdebug"
      },
      {
        "logger": "Ffmpeg",
        "minLevel": "Trace",
        "writeTo": "ffmpegDebug"
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "allfile"
      }
    ]
  },
  "Email": "redshot@saritasa.com",
  "FfmpegWindowsDownloadPath": "https://github.com/BtbN/FFmpeg-Builds/releases/download/autobuild-2020-10-11-12-31/ffmpeg-N-99531-g2be3eb7f77-win64-gpl.zip",
  "FormatFileNameTag": "%"
}
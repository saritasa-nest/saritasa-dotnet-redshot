    <UseWindowsForms>true</UseWindowsForms>
    <NoWarn>NU1701;$(NoWarn)</NoWarn>
    <CodeAnalysisRuleSet>../../../tools/Saritasa.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Eto.Platform.Windows" Version="2.5.3" />
    <PackageReference Include="NLog" Version="4.7.2" />
    <PackageReference Include="SkiaSharp.Views" Version="2.80.1" />
    <PackageReference Include="SkiaSharp.Views.WindowsForms" Version="2.80.1" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.118">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Eto.Forms.Controls.SkiaSharp\Eto.Forms.Controls.SkiaSharp.csproj" />
  </ItemGroup>

</Project>
